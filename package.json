    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "next": "16.1.6",
    "react": "^19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.3",
    "react-use-measure": "^2.1.7",
    "tailwind-merge": "^3.4.0",
    "wrangler": "^4.56.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
